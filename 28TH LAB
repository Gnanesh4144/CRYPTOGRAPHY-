#include <stdio.h>
#include <stdlib.h>
#include <time.h>
int modExp(int base, int exp, int mod) {
    int result = 1;
    base = base % mod;
    while (exp > 0) {
        if (exp % 2 == 1)
            result = (result * base) % mod;
        exp = exp >> 1;
        base = (base * base) % mod;
    }
    return result;
}
int main() {
    int q = 23;     
    int a = 5;       
    int x = 6;       
    int y = 15;       
    int A = modExp(a, x, q);
    int B = modExp(a, y, q);
    int K_alice = modExp(B, x, q);
    int K_bob = modExp(A, y, q);
    printf("Public values: a = %d, q = %d\n", a, q);
    printf("Alice sends: %d\n", A);
    printf("Bob sends: %d\n", B);
    printf("Shared secret key computed by Alice: %d\n", K_alice);
    printf("Shared secret key computed by Bob:   %d\n", K_bob);
    return 0;
}
